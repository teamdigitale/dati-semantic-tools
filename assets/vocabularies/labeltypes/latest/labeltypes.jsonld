      "atold:op-code": "FORMLABEL",
      "dc:identifier": "FORMLABEL",
      "skos:definition": {
        "@language": "en",
        "@value": "Form was a label type in eProcurement-related authority tables to separate a label used in forms from preferred labels."
      },
      "skos:inScheme": {
        "@id": "atold:label-type"
      },
      "skos:prefLabel": {
        "@language": "en",
