    {
      "@id": "http://publications.europa.eu/resource/authority/label-type/ORTHOGRAPHICAL",
      "@type": "skos:Concept",
      "at:authority-code": "ORTHOGRAPHICAL",
      "at:deprecated": "false",
      "at:op-code": "ORTHOGRAPHICAL",
      "at:start.use": "2021-06-16",
      "atold:op-code": "ORTHOGRAPHICAL",
      "dc:identifier": "ORTHOGRAPHICAL",
      "skos:definition": {
        "@language": "en",
        "@value": "Orthographical is a special label type which can be combined with other label types. It can contain HTML tags. It aims to keep italic markings which have a meaning in some languages to separate foreign language elements in a term. This type is not used yet."
      },
      "skos:example": {
        "@language": "en",
        "@value": "Example: in the Latvian version of Euratom Supply Agency, ‘Euratom Apgādes aģentūra’ the first word, Euratom is written with italic to show that this word comes from another language without translation."
      },
      "skos:inScheme": {
        "@id": "atold:label-type"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "orthographical"
      },
      "skos:topConceptOf": {
        "@id": "atold:label-type"
      }
    },
    {
