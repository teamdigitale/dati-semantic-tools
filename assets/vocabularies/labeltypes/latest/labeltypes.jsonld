    {
      "@id": "http://publications.europa.eu/resource/authority/label-type/SYMBOL",
      "@type": "skos:Concept",
      "at:authority-code": "SYMBOL",
      "at:deprecated": "false",
      "at:op-code": "SYMBOL",
      "at:start.use": "2019-02-20",
      "atold:op-code": "SYMBOL",
      "dc:identifier": "SYMBOL",
      "skos:definition": {
        "@language": "en",
        "@value": "Symbol is a label type used in the Measurement unit authority table for displaying the symbols of different measurement units. These symbols and abbreviations can be different from SI (International System of Units) symbols in languages that use other than Latin alphabet."
      },
      "skos:example": {
        "@language": "en",
        "@value": "Example: ‘m’ is the symbol used for ‘metre’ in English."
      },
      "skos:inScheme": {
        "@id": "atold:label-type"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "symbol"
      },
