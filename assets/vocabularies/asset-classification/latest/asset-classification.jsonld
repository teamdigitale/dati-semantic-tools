      "skos:broader": {
        "@id": "http://publications.europa.eu/resource/authority/asset-classification/c_c2f98e5f"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A topic map is a standard for the representation and interchange of knowledge formalised under ISO/IEC 13250:2003 specification. "
      },
      "skos:inScheme": {
        "@id": "atold:asset-classification"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Topic map"
      },
      "skos:scopeNote": {
        "@language": "en",
        "@value": "A topic map represents information using (a) topics, representing any concept, from people, countries, and organisations to software modules, individual files, and events, (b) associations, representing hypergraph relationships between topics, and (c) occurrences, representing information resources relevant to a particular topic."
      }
    },
    {
      "@id": "http://publications.europa.eu/resource/authority/asset-classification/c_366e3b24",
      "@type": "skos:Concept",
      "at:authority-code": "c_366e3b24",
