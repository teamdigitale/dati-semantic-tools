      },
      "skos:definition": {
        "@language": "en",
        "@value": "Business Process Model and Notation (BPMN) is a graphical representation for specifying business processes in a business process model."
      },
      "skos:inScheme": {
        "@id": "atold:asset-classification"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Business Process Model and Notation"
      }
    },
    {
