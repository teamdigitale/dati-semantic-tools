      "skos:broader": {
        "@id": "http://publications.europa.eu/resource/authority/asset-classification/c_64714767"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "Categorisation schemes represent loosely formed grouping schemes."
      },
      "skos:inScheme": {
        "@id": "atold:asset-classification"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Categorisation"
      }
    },
    {
      "@id": "http://publications.europa.eu/resource/authority/asset-classification/c_d5c26195",
      "@type": "skos:Concept",
      "at:authority-code": "c_d5c26195",
