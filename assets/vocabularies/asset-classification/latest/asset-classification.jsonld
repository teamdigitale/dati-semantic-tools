      "skos:broader": {
        "@id": "http://publications.europa.eu/resource/authority/asset-classification/c_722cef3f"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "The presentation style sheet expresses the presentation of structured documents."
      },
      "skos:example": {
        "@language": "en",
        "@value": "One modern style sheet language with widespread use is Cascading Style Sheets (CSS), which is used to style documents written in HTML, XHTML, SVG, XUL and other markup languages."
      },
      "skos:inScheme": {
        "@id": "atold:asset-classification"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Presentation style sheet"
      },
      "skos:scopeNote": {
        "@language": "en",
        "@value": "One attractive feature of structured documents is that the content can be reused in many contexts and presented in various ways. Different style sheets can be attached to the logical structure to produce different presentations."
      }
    },
