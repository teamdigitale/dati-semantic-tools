        }
      ],
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Conceptual model"
      },
      "skos:scopeNote": {
        "@language": "en",
        "@value": "The term conceptual model may be used to refer to models which are formed after a conceptualisation or generalisation process. Conceptual models are often abstractions of things in the real world whether physical or social."
      },
      "skos:topConceptOf": {
