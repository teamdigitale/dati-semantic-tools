      "skos:broader": {
        "@id": "http://publications.europa.eu/resource/authority/asset-classification/c_c2f98e5f"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A mind map is a diagram used to visually organise information in informal hierarchical structures and show relationships among pieces of the whole."
      },
      "skos:inScheme": {
        "@id": "atold:asset-classification"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Mind map"
      },
      "skos:scopeNote": {
        "@language": "en",
        "@value": "A mind map is hierarchical and shows relationships among pieces of the whole. It is often created around a single concept, drawn as an image in the center of a blank page, to which associated representations of ideas such as images, words and parts of words are added. Major ideas are connected directly to the central concept, and other ideas branch out from those major ideas."
      }
    },
    {
      "@id": "http://publications.europa.eu/resource/authority/asset-classification/c_722cef3f",
      "@type": "skos:Concept",
      "at:authority-code": "c_722cef3f",
      "at:deprecated": "false",
      "at:op-code": "c_722cef3f",
      "at:start.use": "2019-12-11",
      "atold:op-code": "c_722cef3f",
      "dc:identifier": "c_722cef3f",
      "skos:altLabel": {
        "@language": "en",
        "@value": "Datamodel"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A data model is an abstract model that organises elements of data and standardises how they relate to one another and to the properties of real-world entities."
      },
      "skos:editorialNote": {
        "@language": "en",
        "@value": "Data model is a formal (syntactic) data structure specification having a deontological or normative nature. It is based upon a closed-world assumption, which means that no uncertainty is assumed and, hence, reasoning is not necessary because facts are simply stated."
      },
      "skos:inScheme": {
        "@id": "atold:asset-classification"
      },
      "skos:narrower": [
